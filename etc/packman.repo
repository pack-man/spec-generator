[Packman]
name = Packman
baseurl = http://repo.packdaddy.io/rpm/$basearch/
gpgkey = http://repo.packdaddy.io/rpm/RPM-GPG-KEY-Packman
gpgcheck = 1
enabled = 1

[Packman-noarch]
name = Packman arch independent
baseurl = http://repo.packdaddy.io/rpm/noarch
gpgkey = http://repo.packdaddy.io/rpm/RPM-GPG-KEY-Packman
gpgcheck = 1
enabled = 1
